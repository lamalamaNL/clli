{
    "name": "lamalama/clli",
    "description": "CLLI - Lama Lama Command Line Interface",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark de Vries",
            "email": "mark@lamalama.nl"
        },
        {
            "name": "Edwin Fennema",
            "email": "edwin@lamalama.nl"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/filesystem": "^10.20",
        "illuminate/support": "^10.20",
        "joetannenbaum/chewie": "^0.1.2",
        "laravel/forge-sdk": "^3.14",
        "laravel/prompts": "^0.1",
        "symfony/console": "^6.0",
        "symfony/process": "^6.0",
        "workerman/workerman": "^4.1"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.3"
    },
    "bin": [
        "bin/clli"
    ],
    "autoload": {
        "psr-4": {
            "LamaLama\\Clli\\Console\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
